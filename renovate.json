{
  "enabled": true,
  "semanticCommits": "enabled",
  "dependencyDashboard": true,
  "dependencyDashboardTitle": "ðŸ¤– Renovate Dashboard ðŸ¤–",
  "rebaseWhen": "conflicted",
  "packageRules": [
    {
      "matchManagers": [
        "github-actions"
      ],
      "addLabels": [
        "renovate/github-action"
      ]
    },
    {
      "matchUpdateTypes": [
        "major"
      ],
      "labels": [
        "dep/major"
      ]
    },
    {
      "matchUpdateTypes": [
        "minor"
      ],
      "labels": [
        "dep/minor"
      ]
    },
    {
      "matchUpdateTypes": [
        "patch"
      ],
      "labels": [
        "dep/patch"
      ],
      "automerge": true
    }
  ]
}
